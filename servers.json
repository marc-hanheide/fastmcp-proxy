{
  "mcpServers": {
    "context7": {
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "time": {
      "transport": "stdio",
      "command": "uvx",
      "args": ["mcp-server-time", "--local-timezone", "Etc/UTC"]
    },
    "mapbox": {
      "timeout": 60,
      "command": "npx",
      "args": [ "-y", "@mapbox/mcp-server"]
    }

  }
}